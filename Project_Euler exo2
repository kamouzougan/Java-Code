// This code will calculate the sum of the all multiples of 2 and 5 between 0-1000 

public class MultipleOf3And5 {
	public static void main(String[]args) 
	{   
		 int total = 0;
		for (int i = 0 ;i < 1000 ;i++) 
		{
			
			if ((i%3== 0) || (i%5==0) )
			{
				
				total += i ;
				
			}
			
	}
		System.out.println(total);
}
}
